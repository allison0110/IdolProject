<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

    <mapper namespace="aDAO">
    	
    	<select id="admin_check" parameterType="String" resultType="aDTO">
    		select * from admin where admin_id = #{id}
    	</select>
    	
    	<insert id="test2" parameterType="Map">
    		insert into test values(#{map})
    	</insert>
    	
    	<insert id="test" parameterType="com.idol.model.TestDTO">
    		insert into test values(#{good}, #{good3}, #{good2})
    	</insert>
    	
    	<insert id="happy" parameterType="com.idol.model.HappyDTO">
    		insert into happy values(#{happy1}, #{happy2}, #{happy3})
    	</insert>
    	
    	<insert id="jong" parameterType="com.idol.model.TestDTO">
    		insert into happy values('', #{good3}, #{good2})
    	</insert>
    	
    	
    </mapper>