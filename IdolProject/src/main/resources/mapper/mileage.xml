<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="mileDAO" >

	<!-- 회원NO에 해당하는 마일리지 내용  -->
	<select id="memMileage" parameterType="memberDTO" resultType="mileDTO">
		select * from mileage_master where mileage_memno = #{member_no}
	</select>
</mapper>